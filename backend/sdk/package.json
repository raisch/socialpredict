{
  "name": "@socialpredict/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript SDK for SocialPredict API",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "docs": "jsdoc -c jsdoc.conf.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "socialpredict",
    "api",
    "sdk",
    "prediction-market",
    "betting"
  ],
  "author": "SocialPredict Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raisch/socialpredict.git"
  },
  "bugs": {
    "url": "https://github.com/raisch/socialpredict/issues"
  },
  "homepage": "https://github.com/raisch/socialpredict#readme",
  "dependencies": {
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "axios-mock-adapter": "^1.22.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "rollup": "^4.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    }
  }
}