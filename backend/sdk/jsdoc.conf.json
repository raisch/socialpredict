{
  "plugins": [
    "node_modules/jsdoc/plugins/markdown.js"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "./src/"
    ],
    "includePattern": "\\.(js)$",
    "exclude": [
      "node_modules/",
      "__tests__/"
    ]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [ "jsdoc", "closure" ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/api/",
    "recurse": true
  }
}